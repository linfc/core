#!/bin/sh
export SIREUM_DIST=true
export SIREUM_HOME=`dirname $(cd "${0%/*}" 2>/dev/null; echo "$PWD"/"${0##*/}")`
if [ -f $0.jar ]; then
  if [ $0 -nt $0.jar ]; then
    echo "Please wait while Sireum is loading..."
    echo
  fi
else
  echo "Please wait while Sireum is loading..."
  echo
fi
${SCALA_BIN:=scala} -nocompdaemon -savecompiled $SCALA_OPTIONS $0 $SIREUM_HOME "$@"
code=$?
if [[ $code == 8 ]]; then
  mv -f $0.new $0
  chmod +x $0
  echo "Reloading Sireum..."
  echo
  exec $0 update
fi
exit $code 
!#
SireumDistro.main(argv)
